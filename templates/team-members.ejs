<ul>
  <% items.forEach(function(member) { %>
    <% if (member.isAlumni === false && 
          (member.labPosition.toLowerCase().includes('phd') || 
           (member.positionType && member.positionType.toLowerCase() === 'staff'))) { %>
      <li>
        <div class="profile-image mb-1" style="background-image:url('<%= member.photoUrl %>');"></div>
        <h3 class="fs-5" style="margin:0;"><%= member.name %></h3>
        <div class="fw-bold" style="font-size: 12px;"><%= member.labPosition %></div>
        <div class="font-monospace" style="font-size: 12px;"><%= member.researchInFiveWords %></div>
        <ul class="social-links">
          <% if (member.github) { %>
            <li><a href="<%= member.github %>">GitHub</a></li>
          <% } %>
          <% if (member.twitter) { %>
            <li><a href="<%= member.twitter %>">Twitter</a></li>
          <% } %>
          <!-- Repeat for other social media links as needed -->
        </ul>
      </li>
    <% } %>
  <% }); %>
</ul>
